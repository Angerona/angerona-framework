<simulation-configuration>
	<name>OCF-Students</name>
    <behavior>angerona.fw.def.DefaultBehavior</behavior>
	<agent-instance>
		<name>Student</name>
        <default-suffix>student</default-suffix>
         
        <config class="angerona.fw.serialize.AgentConfigImport">
			<source>config/agents/default_agent.xml</source>
        </config>
         
        <beliefbase-config class="angerona.fw.serialize.BeliefbaseConfigImport">
			<source>config/beliefbases/ocf_beliefbase.xml</source>
        </beliefbase-config>
        
        <skill>Answer</skill>
        
        <data key="Confidential">
            (Girlfriend, __DEFAULT__, lan)
        </data>
	</agent-instance>
      
	<agent-instance>
      	<name>Girlfriend</name>
      	<default-suffix>girlfriend</default-suffix>
      	
      	<config class="angerona.fw.serialize.AgentConfigImport">
      		<source>config/agents/default_agent.xml</source>
      	</config>
      	
      	<desire>q_Student(lan)</desire>
      	
      	<skill>Query</skill>
      	
      	<beliefbase-config class="angerona.fw.serialize.BeliefbaseConfigImport">
            <source>config/beliefbases/ocf_beliefbase.xml</source>
         </beliefbase-config>
         
	</agent-instance>
</simulation-configuration>
