<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>

  <artifactId>defendingagent</artifactId>
  <packaging>jar</packaging>

  <name>DefendingAgent</name>
  <url>http://maven.apache.org</url>

  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
  </properties>
  
  <build>
    <plugins>
  <plugin>
   <groupId>org.apache.maven.plugins</groupId>
   <artifactId>maven-resources-plugin</artifactId>
   <version>2.5</version>
   <executions>
    <execution>
            <id>Copy KLM-Files</id>
            <goals>
              <goal>copy-resources</goal>
            </goals>
            <phase>process-resources</phase>
            <configuration>
              <outputDirectory>${basedir}/target/resources/</outputDirectory>
              <resources>
                <resource>
                  <directory>src/main/resources</directory>
                  <filtering>true</filtering>
                </resource>
              </resources>
            </configuration>
    </execution>
   </executions>
  </plugin>
  </plugins>
  </build>

  <dependencies>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>4.12</version>
      <scope>test</scope>
    </dependency>
    <dependency>
    	<groupId>com.github.angerona</groupId>
    	<artifactId>fw</artifactId>
    	<version>1.0.2</version>
    </dependency>
    <dependency>
    	<groupId>com.github.angerona.fw.am</groupId>
    	<artifactId>secrecy</artifactId>
    	<version>1.0.1</version>
    </dependency>
    <dependency>
    	<groupId>net.sf.tweety.logics</groupId>
    	<artifactId>cl</artifactId>
    	<version>1.0.358</version>
    </dependency>
    <dependency>
    	<groupId>com.github.angerona</groupId>
    	<artifactId>conditional</artifactId>
    	<version>1.0.1</version>
    </dependency>
    <dependency>
    	<groupId>se.sics</groupId>
    	<artifactId>jasper</artifactId>
    	<version>1.0.0</version>
    </dependency>
    <dependency>
    	<groupId>net.sf.tweety.logics</groupId>
    	<artifactId>pl</artifactId>
    	<version>1.0.358</version>
    </dependency>
    <dependency>
    	<groupId>com.github.angerona</groupId>
    	<artifactId>plwithknowledge</artifactId>
    	<version>1.0.1</version>
    </dependency>
  </dependencies>
  <parent>
  	<groupId>com.github.angerona</groupId>
  	<artifactId>maven-build-all</artifactId>
  	<version>1.0.1</version>
  </parent>
</project>
