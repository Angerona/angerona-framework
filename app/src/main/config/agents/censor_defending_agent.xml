	<cycle-script source="config/censor_agent_cycle.xml" />
	
	<operation-set operation-type="GenerateOptions">
      <default-operator-cls>angerona.fw.defendingagent.operators.def.GenerateOptionsOperator</default-operator-cls>
   	</operation-set>
	
	<operation-set operation-type="IntentionUpdate">
      <default-operator-cls>angerona.fw.defendingagent.operators.def.IntentionUpdateOperator</default-operator-cls>
   	</operation-set>
   	
	<operation-set operation-type="SubgoalGeneration">
      <default-operator-cls>angerona.fw.defendingagent.operators.def.SubgoalGenerationOperator</default-operator-cls>
   	</operation-set>
   	
	<operation-set operation-type="UpdateBeliefs">
      <default-operator-cls>angerona.fw.defendingagent.operators.def.UpdateBeliefsOperator</default-operator-cls>
   	</operation-set>
   	
	<operation-set operation-type="Violates">
      <default-operator-cls>angerona.fw.example.operators.ViolatesOperator</default-operator-cls>
   	</operation-set>
	
	<component>angerona.fw.logic.Desires</component>
	<component>angerona.fw.am.secrecy.components.SecrecyKnowledge</component>
    <component>angerona.fw.PlanComponent</component>
    <component>angerona.fw.defendingagent.ViewComponent</component>
    <component>angerona.fw.defendingagent.CensorComponent</component>
