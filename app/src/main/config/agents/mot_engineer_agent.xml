<agent-configuration>
	<name>Motivation Engineer</name>
	<cycle-script source="config/island_cycle.xml" />
	
	<operation-set operation-type="PlanCalculation">
      <default-operator-cls>com.github.angerona.fw.motivation.plans.impl.TrailOperator</default-operator-cls>
   	</operation-set>
	
	<operation-set operation-type="GenerateOptions">
      <default-operator-cls>com.github.angerona.fw.motivation.operators.MotivationOperator</default-operator-cls>
   	</operation-set>
   	
   	<operation-set operation-type="Filter">
      <default-operator-cls>com.github.angerona.fw.motivation.operators.temp.FilterOperator</default-operator-cls>
   	</operation-set>
   	
	<operation-set operation-type="ActionSelection">
      <default-operator-cls>com.github.angerona.fw.motivation.operators.temp.ActionSelectionOperator</default-operator-cls>
   	</operation-set>
   	
	<operation-set operation-type="UpdateBeliefs">
       <default-operator-cls>com.github.angerona.fw.island.IslandUpdateBeliefsOperator</default-operator-cls>
   	</operation-set>
   	
	<component>com.github.angerona.fw.motivation.dao.impl.MotiveCouplings</component>
	<component>com.github.angerona.fw.motivation.dao.impl.WeightRanges</component>
	<component>com.github.angerona.fw.motivation.dao.impl.LevelWeights</component>
	<component>com.github.angerona.fw.motivation.dao.impl.MotStructure</component>
	<component>com.github.angerona.fw.logic.Desires</component>
	<component>com.github.angerona.fw.motivation.operators.temp.Intentions</component>
	<component>com.github.angerona.fw.motivation.reliable.impl.ActionSequences</component>
	<component>com.github.angerona.fw.motivation.reliable.impl.TimeSlots</component>
	<component>com.github.angerona.fw.island.comp.Battery</component>
	<component>com.github.angerona.fw.island.comp.Area</component>
	
</agent-configuration>
