<beliefbase-configuration>
   <name>Answer-Set Belief base</name>
   <reasoners>
      <default-operator-cls>com.github.kreaturesfw.asp.logic.AspSkepticalReasoner</default-operator-cls>
      <operator-cls>com.github.kreaturesfw.asp.logic.AspCredulousReasoner</operator-cls>
   </reasoners>
   <change-operators>
      <default-operator-cls>com.github.kreaturesfw.asp.logic.RevisionPreferenceHandling</default-operator-cls>
      <operator-cls>com.github.kreaturesfw.asp.logic.AspExpansion</operator-cls>
   </change-operators>
   <translators>
      <default-operator-cls>com.github.kreaturesfw.asp.logic.AspTranslator</default-operator-cls>
   </translators>
   <discrete-family>
     <operator>com.github.kreaturesfw.asp.logic.AspSkepticalReasoner</operator>
     <operator>com.github.kreaturesfw.asp.logic.AspCredulousReasoner</operator>
   </discrete-family>
   <beliefbase-class>com.github.kreaturesfw.asp.logic.AspBeliefbase</beliefbase-class>
</beliefbase-configuration>
