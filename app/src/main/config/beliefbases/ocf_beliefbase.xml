<beliefbase-configuration>
   <name>Conditional Belief Base</name>
   <reasoners>
      <default-operator-cls>com.github.kreatures.core.logic.ocf.ConditionalReasoner</default-operator-cls>
   </reasoners>
   <change-operators>
      <default-operator-cls>com.github.kreatures.core.logic.ocf.ConditionalRevision</default-operator-cls>
      <operator-cls>com.github.kreatures.core.logic.ocf.ConditionalExpansion</operator-cls>
   </change-operators>
   <translators>
      <default-operator-cls>com.github.kreatures.core.logic.ocf.ConditionalTranslator</default-operator-cls>
   </translators>
   
   <beliefbase-class>com.github.kreatures.core.logic.ocf.ConditionalBeliefbase</beliefbase-class>
</beliefbase-configuration>