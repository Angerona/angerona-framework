<beliefbase-configuration>
   <name>Conditional Belief Base</name>
   <reasoners>
      <default-operator-cls>com.github.kreaturesfw.ocf.logic.ConditionalReasoner</default-operator-cls>
   </reasoners>
   <change-operators>
      <default-operator-cls>com.github.kreaturesfw.ocf.logic.ConditionalRevision</default-operator-cls>
      <operator-cls>com.github.kreaturesfw.ocf.logic.ConditionalExpansion</operator-cls>
   </change-operators>
   <translators>
      <default-operator-cls>com.github.kreaturesfw.ocf.logic.ConditionalTranslator</default-operator-cls>
   </translators>
   
   <beliefbase-class>com.github.kreaturesfw.ocf.logic.ConditionalBeliefbase</beliefbase-class>
</beliefbase-configuration>