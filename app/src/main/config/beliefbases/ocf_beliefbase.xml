   <name>Conditional Belief Base</name>
   <reasoners>
      <default-operator-cls>com.github.angerona.fw.logic.conditional.ConditionalReasoner</default-operator-cls>
   </reasoners>
   <change-operators>
      <default-operator-cls>com.github.angerona.fw.logic.conditional.ConditionalRevision</default-operator-cls>
