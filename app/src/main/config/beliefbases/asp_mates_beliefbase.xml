<beliefbase-configuration>
   <name>Answer-Set Belief base</name>
   <reasoners>
      <default-operator-cls>com.github.kreatures.core.logic.asp.AspSkepticalReasoner</default-operator-cls>
      <operator-cls>com.github.kreatures.core.logic.asp.AspCredulousReasoner</operator-cls>
   </reasoners>
   <change-operators>
      <default-operator-cls>com.github.kreatures.core.logic.asp.RevisionPreferenceHandling</default-operator-cls>
      <operator-cls>com.github.kreatures.core.logic.asp.AspExpansion</operator-cls>
   </change-operators>
   <translators>
      <default-operator-cls>com.github.kreatures.core.logic.asp.MatesTranslate</default-operator-cls>
   </translators>
   <discrete-family>
     <operator>com.github.kreatures.core.logic.asp.AspSkepticalReasoner</operator>
     <operator>com.github.kreatures.core.logic.asp.AspCredulousReasoner</operator>
   </discrete-family>
   <beliefbase-class>com.github.kreatures.core.logic.asp.AspBeliefbase</beliefbase-class>
</beliefbase-configuration>
