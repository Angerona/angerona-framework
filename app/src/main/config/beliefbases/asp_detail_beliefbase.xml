<beliefbase-configuration>
   <name>Answer-Set Belief Base (detail)</name>
   <reasoners>
      <default-operator-cls>com.github.kreatures.core.logic.asp.AspReasoner{d=1.0}</default-operator-cls>
   </reasoners>
   <change-operators>
      <default-operator-cls>com.github.kreatures.core.logic.asp.RevisionPreferenceHandling</default-operator-cls>
      <operator-cls>com.github.kreatures.core.logic.asp.AspExpansion</operator-cls>
   </change-operators>
   <translators>
      <default-operator-cls>com.github.kreatures.core.logic.asp.AspTranslator</default-operator-cls>
   </translators>
   
   <continuous-family min-value="0" max-value="1.0">
      <operator>com.github.kreatures.core.logic.asp.AspReasoner</operator>
      <iterator>com.github.kreatures.core.logic.asp.AspIteratorStrategy</iterator>
      <parameter>d</parameter>
   </continuous-family>
   
   <beliefbase-class>com.github.kreatures.core.logic.asp.AspBeliefbase</beliefbase-class>
</beliefbase-configuration>